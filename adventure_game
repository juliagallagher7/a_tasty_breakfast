room_list=[]

room = ["You're in a hallway"
        "There is a bedroom to the south,"
        "and dining room to the east.", 1, None, None, None]

room = ["You're in a dining room"
        "There is a kitchen to the south"
        "And a hall to the west", None, 2, None, None]

room_list.append(room)

room = [You"]
room_list.append(room)

room = [""]
room_list.append(room)

room = [""]
room_list.append(room)

current_room = 0

done = False
while not done:
    print(room_list[current_room][0])
    userInput = input("Which direction:?")

    if userInput.lower() = 'n':
        next_room= room_list[current_room[1]]
        current_room = next_room

    elif userInput.lower() == "w":
        next_room = room_list[current_room[2]]
        current_room = next_room

    elif userInput.lower() == "s":
        next_room = room_list[current_room[3]]
        current_room = next_room


    elif userInput.lower() == "q":
        break
